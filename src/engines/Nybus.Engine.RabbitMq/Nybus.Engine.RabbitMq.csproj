    <RootNamespace>Nybus</RootNamespace>
    <AssemblyName>Nybus.Engine.RabbitMq</AssemblyName>
    <Description>Engine for Nybus based on RabbitMQ.</Description>
    <PackageTags>dotnet-standard;framework;queue;rx;nybus;rabbitmq</PackageTags>
  </PropertyGroup>

  <ItemGroup>
